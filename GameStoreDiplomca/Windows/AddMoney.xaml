<Window x:Class="GameStoreDiplomca.Windows.AddMoney"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameStoreDiplomca.Windows"
        mc:Ignorable="d"
        Title="AddMoney" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="AddMoney_Text" HorizontalAlignment="Center" Margin="0,79,0,0" TextWrapping="Wrap" Text="How many money do you want add to ypur account?" VerticalAlignment="Top" FontFamily="Segoe UI Black" FontSize="18"/>
        <TextBox x:Name="AddMoney_TextBox" HorizontalAlignment="Center" Margin="0,174,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Height="43"/>
        <Button x:Name="AddMoney_Button" Content="AddMoney" HorizontalAlignment="Center" Margin="0,272,0,0" VerticalAlignment="Top" Height="40" Width="104" Click="AddMoney_Button_Click"/>
        <TextBlock x:Name="UserName_Text" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="Money_Text" HorizontalAlignment="Left" Margin="64,10,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>

    </Grid>
</Window>
